    value="-CCACTTCTTATTTACCGAGAAAACT-");

@data
Sequence seq11 = Sequence(taxon="M_sylvanus",
    value="-CCACTTCTTATTTACCGAGAAAACT-");

@data
Sequence seq12 = Sequence(taxon="Saimiri_sciureus",
    value="-TAACTTCTTAATTACCGAGAAAGTG-CGCAAGAACTGCTAATTCATGCTCCCAAGACTAACAACTTGGCTTCCTCAACTTTTAAAGGATAGTAGTTATCCATTGGTCTTAGGAGCCAAAAACATTGGTGCAACTCCAAATAAAAGTAATA---ATACACTTCTCCATCACTCTAATAACACTAATTAGCCTACTAGCGCCAATCCTAGCTACCCTCATTAACCCTAACAAAAGCACACTATACCCGTACTACGTAAAACTAGCCATCATCTACGCCCTCATTACCAGTACCTTATCTATAATATTCTTTATCCTTACAGGCCAAGAATCAATAATTTCAAACTGACACTGAATAACTATCCAAACCATCAAACTATCCCTAAGCTT");

@data
Sequence[] sequences = [seq1, seq2, seq3, seq4, seq5, seq6, seq7, seq8, seq9, seq10, seq11, seq12];

@data
Alignment primates = Alignment(
    sequence=sequences,
    dataType="nucleotide"
);

// The rest of the model remains the same, but with the directly constructed alignment
RealParameter birthRate ~ LogNormalDistributionModel(M=1, S=1);
RealParameter clockRate ~ LogNormalDistributionModel(M=-1, S=1);

String[] calibratedTaxonNames = ["Homo_sapiens", "Pan"];
TaxonSet calibratedTaxa = TaxonSet(taxon=calibratedTaxonNames);
ParametricDistribution lognorm = LogNormalDistributionModel(M=1.7,S=0.1);

Tree tree ~ YuleModel(birthDiffRate=birthRate, taxonset=alignment);
Tree tree ~ MRCAPrior(taxonset=calibratedTaxa, distr=lognorm, monophyletic=false);

StrictClockModel clockModel = StrictClockModel(clock.rate=clockRate);
JukesCantor jc = JukesCantor();

@observed(data=primates)
Alignment alignment ~ TreeLikelihood(tree=tree, siteModel=jc, branchRateModel=clockModel);